(defn interseccion [lista1 lista2]
        (cond
        ((null? lista1) '())
        ((= (first lista1) (first (filter (fn [x] (= x (first lista1))) lista2))) (cons (first lista1) (interseccion (next lista1) lista2)))
        (true interseccion (next lista1) lista2)))
